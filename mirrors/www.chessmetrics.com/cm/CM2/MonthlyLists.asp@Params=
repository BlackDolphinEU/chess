<SCRIPT LANGUAGE=JAVASCRIPT>
 function LinkPlayer(NewPlayerID)
 {
  var x = NewPlayerID + "";
  var y = "000000" + x;
  NewPlayerID = y.substr(y.length-6,6);

  var NewURL = "PlayerProfile.asp?Params=199510SSSSS3S" + NewPlayerID + "000000111000000000000010100";
  location.href = NewURL;
 }
 function LinkMonth(RatingPeriod)
 {
  var x = RatingPeriod;
  var y = RatingPeriod + '000';
  RatingPeriodYear = y.substr(0,4);
  RatingPeriodMonth = y.substr(5,2);
  RatingPeriod = '' + RatingPeriodYear + '' + RatingPeriodMonth

  var NewURL = "SingleMonth.asp?Params=199510SSSSS3S000000" + RatingPeriod + "111000000000000010100";
  location.href = NewURL;
 }
 function ScrollToStartYear(StartYear)
 {
  var NewURL = "MonthlyLists.asp?Params=" + StartYear + "10SSSSS3S000000000000111000000000000010100";
  location.href = NewURL;
 }
 function ZoomToStartYear(StartYear, NewNumYearsOnSummary)
 {
  var NewURL = "MonthlyLists.asp?Params=" + StartYear + NewNumYearsOnSummary + "SSSSS3S000000000000111000000000000010100";
  location.href = NewURL;
 }
 function HandleDurationCombo()
 {
  var x = document.forms[0].elements["DurationCombo"].value;

  
   if (x == "AA")
   {
    ZoomToStartYear("1840","AA");
   }
  

   if (x == "20CC")
   {
    ZoomToStartYear("1900","CC");
   }

   if (x == "19CC")
   {
    ZoomToStartYear("1840","CC");
   }

  
   if (x == "50")
   {
    ZoomToStartYear("1955","50");
   }
  
   if (x == "40")
   {
    ZoomToStartYear("1965","40");
   }
  
   if (x == "30")
   {
    ZoomToStartYear("1975","30");
   }
  
   if (x == "20")
   {
    ZoomToStartYear("1985","20");
   }
  
   if (x == "05")
   {
    ZoomToStartYear("1998","05");
   }
  
   if (x == "03")
   {
    ZoomToStartYear("1999","03");
   }
  
   if (x == "01")
   {
    ZoomToStartYear("2000","01");
   }
  

 }
 function HandleYearStartCombo()
 {
  var x = document.forms[0].elements["YearStartCombo"].value;
  ScrollToStartYear(x);
 }

 function LinkAge(NewAgeInMonths)
 {
  var NewURL = "SingleAge.asp?Params=199510SSSSS3S000000000000111000000000000010" + NewAgeInMonths;
  location.href = NewURL;
 }
 function LinkEvent(NewEventID)
 {
  var x = NewEventID + "";
  var y = "0000000" + x;
  NewEventID = y.substr(y.length-7,7);

  var NewURL = "SingleEvent.asp?Params=199510SSSSS3S000000000000111" + NewEventID + "00000010100";
  location.href = NewURL;
 }
</SCRIPT>

